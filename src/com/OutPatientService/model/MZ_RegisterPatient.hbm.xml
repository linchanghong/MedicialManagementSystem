<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"	
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.OutPatientService.model">
	<class name="MZ_RegisterPatient" table="mz_registerPatient">
		<id name="regiserPatient_id">
			<generator class="native"/>
		</id>
		<property name="regiserPatient_caseNo" not-null="true"/>
		<property name="regiserPatient_name" not-null="true"/>
		<property name="regiserPatient_sex" not-null="true"/>
		<property name="regiserPatient_birthday" not-null="true"/>
		<property name="regiserPatient_address" not-null="true"/>
		<property name="regiserPatient_phoneNo" not-null="true"/>
	</class>
</hibernate-mapping>