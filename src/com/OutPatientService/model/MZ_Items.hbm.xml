<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.OutPatientService.model.MZ_Items" table="MZ_Items">
		<id name="item_id" type="int">
			<generator class="native" />
		</id>
		<property name="item_code" not-null="true" />
		<property name="item_name" not-null="true" />
		<property name="specs" />
        <property name="invoice_N0" />
        <property name="unit_price" />
		<property name="qty" />
        <property name="dept_code" />
        <property name="dept_name" />
		<property name="oper_date" />
		<property name="oper_code" />
		<property name="input_code" />
		<property name="drug_flag" not-null="true" />
	</class>
</hibernate-mapping>