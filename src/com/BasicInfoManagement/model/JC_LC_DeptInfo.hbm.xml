<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"	
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.BasicInfoManagement.model">
	<class name="JC_LC_DeptInfo" table="jc_lc_deptInfo">
		<id name="deptInfo_id">
			<generator class="native"/>
		</id>
		<property name="deptInfo_name" not-null="true"/>
		<property name="deptInfo_number" not-null="true"/>
		<property name="deptInfo_zjm" not-null="true"/>
		
		<!-- 映射与用户的多对一关系 -->
		
	</class>
</hibernate-mapping>