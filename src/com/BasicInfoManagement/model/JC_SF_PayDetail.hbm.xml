<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"	
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.BasicInfoManagement.model">
	<class name="JC_SF_PayDetail" table="jc_sf_payDetail">
		<id name="payDetail_id">
			<generator class="native"/>
		</id>
		<property name="payDetail_number"  not-null="true" />
		<property name="payDetail_name"  not-null="true" />
		
		<property name="payDetail_property"  not-null="true" />
		<property name="payDetail_object"  not-null="true" />
		<property name="payDetail_standard"  not-null="true" />
		<property name="payDetail_price"  not-null="true" />
		<property name="payDetail_remark" type="text"/>
		<property name="payDetail_zjm"  not-null="true" />
	</class>
</hibernate-mapping>
