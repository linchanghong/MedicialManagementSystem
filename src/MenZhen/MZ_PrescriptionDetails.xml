<struts>

<package name="PrescriptionDetails" extends="struts-default" namespace="/PrescriptionDetails">
		<action name="add" class="com.OutPatientService.action.PrescriptionDetailsAction" method="add">
			<result  name="success">/JSP_OutPatientService/Pre_form.jsp</result>
		</action>
		<action name="submit" class="com.OutPatientService.action.PrescriptionDetailsAction" method="submit">
			<result  name="success">/JSP_OutPatientService/Pre_details.jsp</result>
		</action>
		<action name="queryAll" class="com.OutPatientService.action.PrescriptionDetailsAction" method="queryAll">
			<result  name="success">/JSP_OutPatientService/Pre_details.jsp</result>
		</action>
		<action name="modify2" class="com.OutPatientService.action.PrescriptionDetailsAction" method="modify2">
			<result  name="success">/JSP_OutPatientService/Pre_form.jsp</result>
		</action>
		<action name="delete" class="com.OutPatientService.action.PrescriptionDetailsAction" method="delete">
			<result  name="success">/JSP_OutPatientService/Pre_form.jsp</result>
		</action>
</package>

</struts>