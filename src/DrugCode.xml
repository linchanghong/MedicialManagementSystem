<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
   <package name="drugcode" namespace="/DrugCode"  extends="struts-default">
       <action name="drugcode_*" class="com.BasicInfoManagement.action.DrugCodeAction" method="{1}">
         <result name="queryAll">/JSP_BasicInfoMain/DrugCode_index.jsp</result>
         <result name="addsuccess">/JSP_BasicInfoMain/DrugCode_index.jsp</result>
         <result name="deletesuccess">/JSP_BasicInfoMain/DrugCode_index.jsp</result>
         <result name="beforemodify">/JSP_BasicInfoMain/DrugCode_modify.jsp</result>
         <result name="modifysuccess">/JSP_BasicInfoMain/DrugCode_index.jsp</result>
         <result name="qurysuccess">/JSP_BasicInfoMain/DrugCode_index.jsp</result>
         <result name="findAll">/JSP_BasicInfoMain/DrugCode_find.jsp</result>
         <result name="findsuccess">/JSP_BasicInfoMain/DrugCode_find.jsp</result>
       </action>
    </package>
</struts>