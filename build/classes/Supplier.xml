	<package name="Supplier" namespace="/supplier" extends="struts-default">
	
	
	<action name="add" class="com.BasicInfoManagement.action.SupplierAction" method="add">
	<result name="success">/JSP_BasicInfoMain/Supplier_index.jsp</result>
	</action>
	
	<action name="delete" class="com.BasicInfoManagement.action.SupplierAction" method="delete">
	<result name="success">/JSP_BasicInfoMain/Supplier_index.jsp</result>
	</action>
	
	<action name="update" class="com.BasicInfoManagement.action.SupplierAction" method="update">
	<result name="success">/JSP_BasicInfoMain/Supplier_Update.jsp</result>
